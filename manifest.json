{
  "name": "Enola",
  "version": "1.1",
  "manifest_version": 2,
  "description": "This extension will redirect blocked URLs to other URLs.",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": [ "redirect.js" ],
      "run_at": "document_start"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
